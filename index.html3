}
header{
  background:linear-gradient(135deg,#4b0082,#2563eb);
  color:white;
  padding:28px;
  text-align:center;
}
header h1{margin:0;font-size:38px;}
.container{
  max-width:1250px;
  margin:auto;
  padding:20px;
}
.area-title{
  margin-top:45px;
  font-size:28px;
  color:#1e3a8a;
  border-left:6px solid #2563eb;
  padding-left:12px;
}
.hotel{
  background:white;
  border-radius:14px;
  padding:22px;
  margin-top:20px;
  box-shadow:0 6px 18px rgba(0,0,0,0.08);
}
.hotel-head{
  display:flex;
  justify-content:space-between;
  flex-wrap:wrap;
}
.rating{
  background:#ecfeff;
  color:#0369a1;
  padding:6px 12px;
  border-radius:20px;
  font-size:14px;
}
.meta{
  font-size:14px;
  color:#555;
  margin:6px 0;
}
.special{
  background:#fff7ed;
  border-left:4px solid #fb923c;
  padding:10px;
  margin:10px 0;
}
.menu h4{
  margin:12px 0 6px;
  border-bottom:1px solid #e5e7eb;
}
.menu-item{
  display:flex;
  justify-content:space-between;
  padding:5px 0;
}
.price{color:#047857;font-weight:600;}
.book{
  margin-top:14px;
  background:#2563eb;
  color:white;
  border:none;
  padding:10px 18px;
  border-radius:8px;
  cursor:pointer;
}
#searchInput:hover{
  outline:none;
  border-color:#2563eb;
  box-shadow:0 0 6px rgba(37,99,235,0.4);
}

.book:hover{background:#1e40af;}
.about{
  margin-top:60px;
  background:white;
  padding:30px;
  border-radius:14px;
  box-shadow:0 6px 18px rgba(0,0,0,0.08);
}
.team{
  display:grid;
  grid-template-columns:repeat(auto-fit,minmax(180px,1fr));
  gap:20px;
}
.member{
  background:#eef2ff;
  padding:18px;
  text-align:center;
  border-radius:12px;
}
.member i{
  font-size:36px;
  color:#4b0082;
  margin-bottom:10px;
}
footer{
  margin-top:40px;
  background:#1e293b;
  color:white;
  text-align:center;
  padding:16px;
}
.map-btn{
  margin-left:10px;
  background:#16a34a;
  color:white;
  border:none;
  padding:8px 14px;
  border-radius:8px;
  cursor:pointer;
  font-size:14px;
}
.map-btn:hover{
  background:#15803d;
}

</style>
</head>

