const outputDiv = document.getElementById("output");

for (let areaName in restaurantData) {

 
  const areaTitle = document.createElement("div");
  areaTitle.className = "area-title";
  areaTitle.innerText = areaName;
  outputDiv.appendChild(areaTitle);


  restaurantData[areaName].forEach(function(hotel){

    let menuHTML = "";

 
    for (let section in hotel.menu) {
      menuHTML += "<div class='menu'><h4>" + section + "</h4>";

      hotel.menu[section].forEach(function(item){
        menuHTML +=
          "<div class='menu-item'>" +
          "<span>" + item[0] + "</span>" +
          "<span class='price'>₹" + item[1] + "</span>" +
          "</div>";
      });

      menuHTML += "</div>";
    }
    const hotelCard = document.createElement("div");
hotelCard.className = "hotel";
hotelCard.setAttribute("data-name", hotel.name.toLowerCase());

    hotelCard.innerHTML = `
      <div class="hotel-head">
        <h3>${hotel.name}</h3>
        <span class="rating">⭐ ${hotel.rating}</span>
      </div>
      <div class="meta"><i class="fas fa-clock"></i> ${hotel.hours}</div>
        <div class="meta">
    <div class="meta">
  <i class="fas fa-location-dot"></i> ${hotel.address}
</div>

  </div>
      <div class="special"><strong>Special:</strong> ${hotel.special}</div>
      ${menuHTML}
      <div style="margin-top:14px;">
  <button class="book">Book Now</button>
  <button class="map-btn">View on Map</button>
</div>

    `;

   
    hotelCard.querySelector(".book").onclick = function(){
      const confirmBooking = confirm("Do you want to book a table at " + hotel.name + "?");
      if(confirmBooking){
        alert("Thank you! Booking feature will be enabled soon.");
      }
    };
    hotelCard.querySelector(".map-btn").onclick = function(){
      const confirmBooking = confirm("Do U Want to see the location in G-map for " + hotel.name + "?");
      if(confirmBooking){
      
        window.open(
  "https://www.google.com/maps/search/?api=1&query=" + encodeURIComponent(hotel.address),
  "_blank"
);

      }
    };
    outputDiv.appendChild(hotelCard);
  });
}
//search bar functioning is written in js
document.getElementById("searchInput").addEventListener("keyup", function () {

  const searchValue = this.value.toLowerCase();
  const allHotels = document.querySelectorAll(".hotel");

  allHotels.forEach(function(hotel){
    const hotelName = hotel.getAttribute("data-name");

    if (hotelName.includes(searchValue)) {
      hotel.style.display = "block";
    } else {
      hotel.style.display = "none";
    }
  });

});

